//calculate the area of rectangle Option A

function areaofrectangle(base,height){
    return base*height
}

console.log(areaofrectangle(10,20))
console.log(areaofrectangle(30,40))
console.log(areaofrectangle(5.0))


//calculate the area of rectangle Option B

const base1=10
const height1=20
console.log(base1*height1)

const base2=30
const height2=40
console.log(base2*height2)

const base3=5
const heigh3=0
console.log(base3*heigh3)


